{
  "name": "job-scraper",
  "version": "1.0.0",
  "description": "A serverless application for crawling tech job websites",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start" : "node src/index.js",
    "docker:build": "docker build . --tag gcr.io/jobjawn-scraper/jobjawn",
